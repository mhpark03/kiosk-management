# Backend API URL
VITE_API_URL=http://localhost:8080/api

# Firebase Configuration
# Get these from: Firebase Console > Project Settings > General > Your apps > SDK setup and configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890

# Runway ML API Configuration (Optional)
# Get your API key from: https://dev.runwayml.com
VITE_RUNWAY_API_KEY=your_runway_api_key_here

# Optional: Runway API URL (defaults to Runway's official endpoint)
# VITE_RUNWAY_API_URL=https://api.runwayml.com/v1
